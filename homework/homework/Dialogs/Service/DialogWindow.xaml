<Window x:Class="_2Term_OOP_Lab5.LoadScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_2Term_OOP_Lab5"
        xmlns:viewModels="clr-namespace:_2Term_OOP_Lab5"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        SizeToContent="WidthAndHeight"
        Title="{Binding Title}" Height="300" Width="300">
        <ContentControl x:Name="ContentPresenter" Content="{Binding}"/>
</Window>
