<Application x:Class="_2Term_OOP_Lab5.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:_2Term_OOP_Lab5"
             xmlns:alert="clr-namespace:_2Term_OOP_Lab5.Dialogs.AlertDialog"
             xmlns:yesno="clr-namespace:_2Term_OOP_Lab5.Dialogs.YesNoDialog"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <DataTemplate DataType="{x:Type local:AlertDialogViewModel}">
             <alert:AlertDialogView/>
         </DataTemplate>
        <DataTemplate DataType="{x:Type local:YesNoDialogViewModel}">
            <yesno:YesNoView/>
        </DataTemplate>
    </Application.Resources>
</Application>
